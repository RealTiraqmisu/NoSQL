<%- include("head") %>
<body>
    <h1>Users List</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach((item)=>{ %>
                <tr>
                    <td><%=item.id%></td>
                    <td><%=item.first_name%></td>
                    <td><%=item.last_name%></td>
                    <td><%=item.username%></td>
                    <td><%=item.email%></td>
                    <td>
                        <div id="btn">
                            <a href="/userdetail?id=<%=item.id%>" id="foruserlist">Detail</a>
                        </div>
                    </td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>